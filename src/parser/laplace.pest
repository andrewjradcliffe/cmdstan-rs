laplace = { "laplace" ~ ((" ")+ ~ laplace_term)* }

laplace_term = {
    mode
    | jacobian
    | draws
}

mode = ${
    !(("mode" ~ ((" ")+ ~ "mode" ~ (&(" ") | EOI))+))
    ~ "mode" ~ ("=" ~ path)?
}
draws = ${
    !(("draws" ~ ((" ")+ ~ "draws" ~ (&(" ") | EOI))+))
    ~ "draws" ~ ("=" ~ integer)?
}
